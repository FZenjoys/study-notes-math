number = {2},
pages = {262},
title = {{Application of Screw Theory to Rigid Body Dynamics}},
url = {http://cat.inist.fr/?aModele=afficheN{\&}cpsidt=5369841 http://dynamicsystems.asmedigitalcollection.asme.org/article.aspx?articleid=1405915},
volume = {114},
