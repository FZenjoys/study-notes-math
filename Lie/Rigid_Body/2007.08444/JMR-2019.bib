journal = {Robotica},
keywords = {Functional electrical stimulation (FES),Kinematic modeling and control for pHRI,Physical human-robot interaction (pHRI)},
number = {2},
pages = {314--331},
title = {{Kinematic modeling and control for human-robot cooperation considering different interaction roles}},
volume = {33},
year = {2015}
}
